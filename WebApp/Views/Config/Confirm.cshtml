@model WebApp.Models.ConfigModel

@{
    ViewBag.Title = "Confirm";
}
<h2>Are you sure you want to delete the handler?</h2>
<h2> @Model.DeleteHandler </h2>
<button type="button" id="btnOK" onclick="location.href='@Url.Action("ApprovedDelete", "Config")'" style="margin:10px">OK</button>
<button type="button" id="btnCancel" onclick="location.href='@Url.Action("CanceledDelete", "Config")'" style="margin:10px">Cancel</button>
